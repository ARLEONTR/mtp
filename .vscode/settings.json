{
    "editor.formatOnSave": true,
    "clang-format.style": ".clang-format",
    "C_Cpp.clang_format_path": "/usr/bin/clang-format",
    "C_Cpp.codeFolding": "enabled",
    "C_Cpp.default.includePath": [
        "${workspaceFolder}",
        "${workspaceFolder}/mtpv1",
        "${workspaceRoot}/linux-6.6.2/**",
        "${workspaceFolder}/linux-6.6.2/include",
        "${workspaceFolder}/linux-6.6.2/include/generated",
        "${workspaceFolder}/linux-6.6.2/arch/x86/include",
        "${workspaceFolder}/linux-6.6.2/arch/x86/include/generated",
        "${workspaceFolder}/linux-6.6.2/arch/x86/include/generated/uapi",
        "${workspaceFolder}/linux-6.6.2/include/uapi",
        "${workspaceFolder}/linux-6.6.2/arch/x86/include/uapi"
    ],
    "C_Cpp.default.compilerPath":  "/usr/bin/gcc",
    "C_Cpp.default.intelliSenseMode": "linux-gcc-x64",
    "C_Cpp.default.defines": [
        "KBUILD_MODNAME=\"mtp\"",
        "__GNUC__",
        "__KERNEL__",
        "MODULE"
    ],
    "C_Cpp.default.cStandard": "gnu11",
    "C_Cpp.default.cppStandard": "gnu++17",
    "C_Cpp.default.browse.path": [
        "${workspaceFolder}",
        "${workspaceRoot}/**",
        "${workspaceFolder}/linux-6.6.2/include",
        "${workspaceFolder}/linux-6.6.2/mm",
        "${workspaceFolder}/linux-6.6.2/fs",
        "${workspaceFolder}/linux-6.6.2/kernel",
        "${workspaceFolder}/mtpv1"
    ],
    "C_Cpp.default.browse.limitSymbolsToIncludedHeaders": true,
    "C_Cpp.default.browse.databaseFilename": "",
    "C_Cpp.autocompleteAddParentheses": true,
    "C_Cpp.errorSquiggles": "disabled",
    "files.associations": {
        "mtpimpl.h": "c",
        "bug.h": "c",
        "inet.h": "c"
    }

    
}